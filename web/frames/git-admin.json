{
	"comment": "set the HomeDirectory from sftp pwd",
	"type": "SecureFtp",
	"id": 411000,
	"sftp_script": "'pwd'",
	"on_complete": {
		"type": "LinearLayout",
		"scrollable": true,
		"orientation": "vertical",
		"gravity": "center_horizontal",
		"layout_width": "match_parent",
		"component_list": [
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "Clone",
				"name": "TextViewLabel",
				"attributes": {
					"text": "Refresh Beta Branch from GITHUB"
				}
			},
			{
				"type": "Clone",
				"name": "TextViewDetail",
				"attributes": {
					"text_list": [
						"Click [Checkout EigenFrame Beta Branch] activates local beta branch",
						"Click [Fetch/Reset GitHub EigenFrame Beta Branch] to do a hard reset",
						"Click [Pull from GitHub] to pull updates for current branch"
					]
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "Checkout EigenFrame Beta Branch",
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "Checkout EigenFrame Beta Branch",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_list": [
														"cd ~/git-repos/eigenframe",
														"echo //git checkout -B beta",
														"git checkout -B beta",
														"echo //git status",
														"git status"
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "Fetch/Reset GITHUB EigenFrame Beta Branch",
					"icon": {
						"name": "cloud_download.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "Fetch/reset GITHUB beta branch - into existing local beta branch",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_list": [
														"cd ~/git-repos/eigenframe",
														"echo //git status",
														"git status",
														"echo //git stash",
														"git stash",
														"echo //git fetch origin",
														"git fetch origin",
														"echo //git reset --hard origin/beta",
														"git reset --hard origin/beta",
														"cd ~/git-repos/eigenframe/cgi-bin",
														"chmod 755 *.sh",
														"chmod 755 *.rb",
														"echo //git status",
														"git status"
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "Pull from GITHUB",
					"icon": {
						"name": "cloud_download.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "Pull from GITHUB",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_list": [
														"cd ~/git-repos/eigenframe",
														"echo //git stash",
														"git stash",
														"echo //git pull",
														"git pull",
														"cd ~/git-repos/eigenframe/cgi-bin",
														"chmod 755 *.sh",
														"chmod 755 *.rb",
														"git status"
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git status",
					"icon": {
						"name": "info.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git status",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh": "cd ~/git-repos/eigenframe ; git status"
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "JavaScript",
				"script_list": [
					"eigenFragment.setCacheString('SelectFilePrompt', 0, 'Select EigenFrame File')",
					"eigenFragment.setCacheString('WorkingDirectory', 'git-repos/eigenframe')",
					"eigenFragment.setCacheString('SelectedPath', '')",
					"eigenFragment.setCacheString('SelectedFile', '')"
				]
			},
			{
				"type": "Clone",
				"name": "JavaScriptSelectFilePath"
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "Clone",
				"name": "TextViewLabel",
				"attributes": {
					"text": "Check differences between selected file and master: "
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git diff from master",
					"icon": {
						"name": "check.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git diff from master",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_script_list": [
														"var selected_path = eigenFragment.getCacheString('SelectedPath')",
														"var selected_file = eigenFragment.getCacheString('SelectedFile')",
														"var git_filepath = eigenFragment.concatPaths(selected_path, selected_file)",
														"'cd ~/git-repos/eigenframe ; git diff master:' + git_filepath + ' -- ' + git_filepath "
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git restore (revert to master)",
					"icon": {
						"name": "undo.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git restore",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_script_list": [
														"var selected_path = eigenFragment.getCacheString('SelectedPath')",
														"var selected_file = eigenFragment.getCacheString('SelectedFile')",
														"var git_filepath = eigenFragment.concatPaths(selected_path, selected_file)",
														"'cd ~/git-repos/eigenframe ; git restore ' + git_filepath "
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "Clone",
				"name": "TextViewLabel",
				"attributes": {
					"text": "Commit Changes to GITHUB (Beta Branch)"
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git add *",
					"icon": {
						"name": "add.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git add *",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_script": "'cd ~/git-repos/eigenframe ; git add *'"
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git commit -a m 'testing'",
					"icon": {
						"name": "save.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git commit -a m 'testing'",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_script": "'cd ~/git-repos/eigenframe ; git commit -a -m \"testing\"'"
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git push origin beta",
					"icon": {
						"name": "cloud_upload.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git push origin beta",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_script": "'cd ~/git-repos/eigenframe ; git push origin beta' "
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "Clone",
				"name": "TextViewLabel",
				"attributes": {
					"text": "Refresh GIT Working Directory (Beta Branch) from GITHUB Master: "
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "Pull from github master\n(merge to working beta branch)",
					"icon": {
						"name": "cloud_download.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "Pull from github master\n(merge to working beta branch)",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_list": [
														"cd ~/git-repos/eigenframe",
														"echo //git status",
														"git status",
														"echo //git checkout master",
														"git checkout master",
														"echo //git pull",
														"git pull",
														"echo //git checkout beta",
														"git checkout beta",
														"echo //git merge master",
														"git merge master",
														"echo //git status",
														"git status",
														"echo //enable execution of cgi-bin files",
														"cd ~/git-repos/eigenframe/cgi-bin",
														"chmod 755 *.sh",
														"chmod 755 *.rb"
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "HtmlView",
				"layout_width": "match_parent",
				"layout_height": "wrap_content",
				"gravity": "center",
				"x_text_size": "14",
				"text_color": "#ffffff",
				"html_list": [
					"<h1><font color=\"#8aec81\">EigenFrame Github Repository</font></h1>",
					"https://github.com/jsawaya/eigenframe"
				]
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			}
		]
	}
}


