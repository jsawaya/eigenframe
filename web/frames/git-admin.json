{
	"comment": "set the HomeDirectory from sftp pwd",
	"type": "SecureFtp",
	"id": 411000,
	"sftp_script": "'pwd'",
	"on_complete": {
		"type": "LinearLayout",
		"scrollable": true,
		"orientation": "vertical",
		"gravity": "center_horizontal",
		"layout_width": "match_parent",
		"component_list": [
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "Clone",
				"name": "TextViewLabel",
				"attributes": {
					"text": "Refresh GIT Working Directory (test branch) from GITHUB Master: "
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "Pull from github master\n(merge to working test branch)",
					"icon": {
						"name": "cloud_download.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "Pull from github master\n(merge to working test branch)",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_list": [
														"cd ~/git-repos/eigenframe",
														"echo current working directory:",
														"pwd",
														"echo ",
														"echo git status:",
														"git status",
														"echo ",
														"echo git checkout master:",
														"git checkout master",
														"echo ",
														"echo git pull:",
														"git pull",
														"echo ",
														"echo git checkout test:",
														"git checkout test",
														"echo ",
														"echo git merge master:",
														"git merge master",
														"echo ",
														"echo git status:",
														"git status",
														"echo enable execution of cgi-bin directory files",
														"cd ~/git-repos/eigenframe/cgi-bin",
														"chmod 755 *.sh",
														"chmod 755 *.rb"
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "Pull from github beta branch",
					"icon": {
						"name": "cloud_download.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "Pull from github beta branch",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_list": [
														"cd ~/git-repos/eigenframe",
														"echo current working directory:",
														"pwd",
														"echo ",
														"echo git status:",
														"git status",
														"echo ",
														"echo git checkout beta:",
														"git checkout --track origin/beta",
														"echo ",
														"echo git pull:",
														"git pull",
														"echo ",
														"echo git status:",
														"git status",
														"echo enable execution of cgi-bin directory files",
														"cd ~/git-repos/eigenframe/cgi-bin",
														"chmod 755 *.sh",
														"chmod 755 *.rb"
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git status",
					"icon": {
						"name": "info.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git status",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh": "cd ~/git-repos/eigenframe ; git status"
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "Clone",
				"name": "TextViewLabel",
				"attributes": {
					"text": "Select File (in eigenframe git working storage)"
				}
			},
			{
				"type": "EditText",
				"id": 6699,
				"max_length": 200,
				"is_single_line": true,
				"layout_width": "match_parent",
				"text_color": "#ffffff",
				"background_color": "#000000"
			},
			{
				"type": "JavaScript",
				"script_list": [
					"eigenFragment.setIntegerVariable('TargetViewId', 0, 6699)",
					"eigenFragment.setCacheString('WorkingDirectory', 'git-repos/eigenframe')",
					"eigenFragment.setCacheString('SelectedPath', '')",
					"eigenFragment.setCacheString('SelectedFile', '')"
				]
			},
			{
				"type": "Clone",
				"name": "JavaScriptSetFullPath"
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "Select File (in eigenframe git working storage)",
					"icon": {
						"name": "open_with.",
						"location": "top"
					},
					"on_click": {
						"type": "ActionList",
						"comment": "set the dialog type, then start popup",
						"component_list": [
							{
								"type": "JavaScript",
								"script_list": [
									"eigenFragment.setBooleanVariable('IsShowWorkingDirectory',0,true)",
									"eigenFragment.setBooleanVariable('IsShowCurrentFilename',0,true)"
								]
							},
							{
								"type": "Clone",
								"name": "PopupScreenDefault",
								"attributes": {
									"id": 991005,
									"title": "Select File (in eigenframe git working storage)",
									"url_script": "eigenFragment.getCacheString('RootUrl')+'frames/select-filepath.json'"
								}
							}
						]
					}
				}
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "Clone",
				"name": "TextViewLabel",
				"attributes": {
					"text": "Check differences between selected file and master: "
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git diff from master",
					"icon": {
						"name": "check.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git diff from master",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_script_list": [
														"var selected_path = eigenFragment.getCacheString('SelectedPath')",
														"var selected_file = eigenFragment.getCacheString('SelectedFile')",
														"var git_filepath = eigenFragment.concatPaths(selected_path, selected_file)",
														"'cd ~/git-repos/eigenframe ; git diff master:' + git_filepath + ' -- ' + git_filepath "
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git restore (revert to master)",
					"icon": {
						"name": "undo.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git restore",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_script_list": [
														"var selected_path = eigenFragment.getCacheString('SelectedPath')",
														"var selected_file = eigenFragment.getCacheString('SelectedFile')",
														"var git_filepath = eigenFragment.concatPaths(selected_path, selected_file)",
														"'cd ~/git-repos/eigenframe ; git restore ' + git_filepath "
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "Clone",
				"name": "TextViewLabel",
				"attributes": {
					"text": "Commit Changes in GIT Working Directory to GITHUB (Test Branch): "
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git add *",
					"icon": {
						"name": "add.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git add *",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_script": "'cd ~/git-repos/eigenframe ; git add *'"
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git commit -a m 'testing'",
					"icon": {
						"name": "save.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git commit -a m 'testing'",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_script": "'cd ~/git-repos/eigenframe ; git commit -a -m \"testing\"'"
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git push origin test",
					"icon": {
						"name": "cloud_upload.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git push origin test",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_script": "'cd ~/git-repos/eigenframe ; git push origin test' "
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "Clone",
				"name": "TextViewLabel",
				"attributes": {
					"text": "Sign into GITHUB, select Test branch, create pull request, and merge: "
				}
			},
			{
				"type": "HtmlView",
				"layout_width": "match_parent",
				"layout_height": "wrap_content",
				"gravity": "center",
				"x_text_size": "14",
				"text_color": "#ffffff",
				"html_list": [
					"<h1><font color=\"#8aec81\">EigenFrame Github Webpage: </font></h1>",
					"https://github.com/jsawaya/eigenframe"
				]
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			}
		]
	}
}


