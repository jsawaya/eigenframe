{
  "type": "LinearLayout",
  "scrollable": true,
  "orientation": "vertical",
  "gravity": "center_horizontal",
  "layout_width": "match_parent",
  "layout_height": "wrap_content",
  "component_list": [
    {
      "type": "HtmlView",
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "gravity": "center",
      "text_color": "#ffffff",
      "html_list": [
        "<img src='ic_launcher.'>",
        "<h1><font color=\"#8aec81\">EigenFrame: </font></h1>",
        "<h4><font color=\"#8aec81\">Framework for Dynamic Android Apps</font></h4>",
        "{Specification, Deployment, Rendering, Execution}"
      ]
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Launch an app from a local file..."
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text": "The following demonstrates how to launch an app from your local shared directory:"
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_size": "10",
        "text": "/storage/emulated/0/Android/data/com.sawaya.eigenframe/files"
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Hello World",
        "icon": {
          "name": "open_browser.",
          "location": "top"
        },
        "on_click": {
          "type": "JavaScript",
          "script": "eigenActivity.launch('file:///storage/emulated/0/Android/data/com.sawaya.eigenframe/files/yourapp.json')"
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Install EigenFrame Services"
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text": "Click [Install] to prepare Termux App for EigenFrame services."
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Install",
        "icon": {
          "name": "playlist_check.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "Install",
            "url_script": "eigenFragment.getCacheString('RootUrl')+'frames/ssh-initial-setup.json'"
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "EigenFrame GitHub Repository Administration"
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text": "Click [GitHub] to download the latest updates from EigenFrame GitHub Repository."
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "GitHub",
        "icon": {
          "name": "github_logo.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "GitHub",
            "url_script": "eigenFragment.getCacheString('RootUrl')+'frames/ssh-git-admin.json'"
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Secure File Transfer Protocol (SFTP) - Examples"
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text": "Click [SFTP] perform remote file management."
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "SFTP",
        "icon": {
          "name": "import_export.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "SFTP",
            "url_script": "eigenFragment.getCacheString('RootUrl')+'frames/sftp-gallery.json.json'"
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Edit Remote File",
        "icon": {
          "name": "edit.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "Edit Remote File",
            "url_script": "eigenFragment.getCacheString('RootUrl')+'frames/sftp-edit.json'"
          }
        }
