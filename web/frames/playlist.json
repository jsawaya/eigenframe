          "checked_option": 0,
          "icon": {
            "name": "share."
          },
          "id": 999101,
          "on_click": {
            "script_list": [
              "position = eigenFragment.getOptionPosition()",
              "github_branch = 'master' ",
              "if (position == 1) github_branch = 'beta' ",
              "if (position == 2) github_branch = '1.3' ",
              "eigenFragment.setCacheString('GithubBranch', github_branch)",
              "var root_url = 'https://raw.githubusercontent.com/jsawaya/eigenframe/'+github_branch+'/web/'",
              "eigenFragment.setCacheString('RootUrl', root_url)",
              "eigenActivity.showToast('root_url: ' + root_url )",
              "eigenActivity.launch(eigenFragment.getCacheString('RootUrl')+'apps/app_github_master.json')",
              "eigenActivity.finish()"
            ],
            "type": "JavaScript"
          },
          "option_list": [
            "master",
            "beta - initial deployment",
            "version 1.3"
          ],
          "title": "Select Branch",
          "type": "SelectDialog"
