    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Shell Edit/Exec",
        "icon": {
          "name": "play.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "Shell",
            "url_script": "eigenFragment.getCacheString('RootUrl')+'frames/ssh-cmd.json'"
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "EigenFrame Edit/Exec",
        "icon": {
          "name": "ic_launcher_round.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "EigenFrame Edit/Exec",
            "url_script": "eigenFragment.getCacheString('RootUrl')+'frames/sftp-edit-frame.json'"
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Ruby Edit/Exec",
        "icon": {
          "name": "ruby_logo.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "Ruby Edit/Exec",
            "url_script": "eigenFragment.getCacheString('RootUrl')+'frames/ssh-ruby.json'"
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Prolog Edit/Exec",
        "icon": {
          "name": "swi_prolog.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "Prolog Edit/Exec",
            "url_script": "eigenFragment.getCacheString('RootUrl')+'frames/ssh-ruby-swipl.json'"
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "OpenPGP Cryptography"
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_list": [
          "Click [OpenPGP Cryptography] to use GnuPG to encrypt/decrypt/sign/verify/import-keys/export-keys.  "
        ]
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "OpenPGP Cryptography",
        "icon": {
          "name": "security.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "OpenPGP Cryptography",
            "url_script": "eigenFragment.getCacheString('RootUrl')+'frames/ssh-ruby-pgp.json'"
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Apache2 HTTP/CGI (Common Gateway Interface)"
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_list": [
          "Click [Apache2] to use cgi as an alternative to ssh."
        ]
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Apache",
        "icon": {
          "name": "security.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "Apache2",
            "url_script": "eigenFragment.getCacheString('RootUrl')+'frames/ssh-apache.json'"
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "EigenFrame GitHub Gallery"
      }
    },
    {
      "type": "Clone",
      "name": "SetIsMasterBranchButton"
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_list": [
          "Click [Gallery] to see a cloud-based gallery of eigenframe layouts, components and actions;  use tabs to select, or drag left, then click source button to see examples"
        ]
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Gallery",
        "icon": {
          "name": "open_browser.",
          "location": "top"
        },
        "on_click": {
          "type": "JavaScript",
          "script_list": [
            "eigenActivity.launch('https://raw.githubusercontent.com/jsawaya/eigenframe/master/web/apps/app_github_master_gallery.json')"
          ]
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "EigenFrame GitHub Master Branch"
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_list": [
          "Click [Launch GitHub Master Branch] to run EigenFrame apps as Termux services.",
          "Administer SSH/SFTP, GIT, Bash, Ruby, Prolog, PGP, Apache2, ..."
        ]
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Launch GitHub Master Branch",
        "icon": {
          "name": "open_browser.",
          "location": "top"
        },
        "on_click": {
          "type": "JavaScript",
          "script_list": [
            "eigenFragment.setCacheString('GithubBranch', 'master')",
            "eigenFragment.setViewState(999200, '1')",
            "eigenActivity.launch('https://raw.githubusercontent.com/jsawaya/eigenframe/master/web/apps/app_github_master.json')"
          ]
        }
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_list": [
          "Click [Master Branch Shortcut] to create a shortcut on your home screen to launch the EigenFrame app from the github master branch"
        ]
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Master Branch Shortcut",
        "background_color": "#123456",
        "icon": {
          "name": "save.",
          "location": "top"
        },
        "on_click": {
          "type": "JavaScript",
          "script_list": [
            "eigenActivity.addShortcut('Master', 'https://raw.githubusercontent.com/jsawaya/eigenframe/master/web/apps/app_github_master.json')",
            "eigenActivity.finish()"
          ]
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Beta Branch Features"
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_list": [
          "Click [Launch GitHub Beta Branch] for pre-release features (in development)"
        ]
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Launch GitHub Beta Branch",
        "icon": {
          "name": "open_browser.",
          "location": "top"
        },
        "on_click": {
          "type": "JavaScript",
          "script_list": [
            "eigenFragment.setCacheString('GithubBranch', 'beta')",
            "eigenFragment.setViewState(999200, '0')",
            "eigenActivity.launch('https://raw.githubusercontent.com/jsawaya/eigenframe/beta/web/apps/app_github_beta.json')"
          ]
        }
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_list": [
          "Click [Beta Shortcut] to create a shortcut on your home screen to launch the EigenFrame app from the github beta branch"
        ]
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Beta Shortcut",
        "background_color": "#123456",
        "icon": {
          "name": "save.",
          "location": "top"
        },
        "on_click": {
          "type": "JavaScript",
          "script_list": [
            "eigenActivity.addShortcut('Beta', 'https://raw.githubusercontent.com/jsawaya/eigenframe/beta/web/apps/app_github_beta.json')",
            "eigenActivity.finish()"
          ]
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    }

  ]
}