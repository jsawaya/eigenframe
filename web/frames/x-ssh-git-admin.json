{
  "type": "LinearLayout",
  "scrollable": true,
  "orientation": "vertical",
  "gravity": "center_horizontal",
  "layout_width": "match_parent",
  "component_list": [

    {
      "type": "Clone",
      "name": "HorizontalLine"
    },

    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Refresh GIT Working Directory (test branch) from GITHUB Master: "
      }
    },

    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Pull from github master\n(merge to working test branch)",
        "icon": {
          "name": "cloud_download.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "Pull from github master\n(merge to working test branch)",
            "component_list": [
              {
                "type": "Clone",
                "name": "LayoutVerticalScrollable",
                "attributes": {
                  "component_list": [
                    {
                      "type": "Clone",
                      "name": "TextViewDetail",
                      "attributes": {
                        "text_size": "10",
                        "ssh_list": [
                          "cd ~/git-repos/eigenframe",
                          "echo current working directory:",
                          "pwd",
                          "echo ",
                          "echo git status:",
                          "git status",
                          "echo ",
                          "echo git checkout master:",
                          "git checkout master",
                          "echo ",
                          "echo git pull:",
                          "git pull",
                          "echo ",
                          "echo git checkout test:",
                          "git checkout test",
                          "echo ",
                          "echo git merge master:",
                          "git merge master",
                          "echo ",
                          "echo git status:",
                          "git status",
                          "echo enable execution of cgi-bin directory files",
                          "cd ~/git-repos/eigenframe/cgi-bin",
                          "chmod 755 *.sh",
                          "chmod 755 *.rb"
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },
		{
			"type": "Clone",
			"name": "ButtonDefault",
			"attributes": {
				"text": "Pull from github beta\n(merge to working test branch)",
				"icon": {
					"name": "cloud_download.",
					"location": "top"
				},
				"on_click": {
					"type": "Clone",
					"name": "PopupScreenDefault",
					"attributes": {
						"title": "Pull from github beta\n(merge to working test branch)",
						"component_list": [
							{
								"type": "Clone",
								"name": "LayoutVerticalScrollable",
								"attributes": {
									"component_list": [
										{
											"type": "Clone",
											"name": "TextViewDetail",
											"attributes": {
												"text_size": "10",
												"ssh_list": [
													"cd ~/git-repos/eigenframe",
													"echo current working directory:",
													"pwd",
													"echo ",
													"echo git status:",
													"git status",
													"echo ",
													"echo git checkout beta:",
													"git checkout --track origin/beta",
													"echo ",
													"echo git pull:",
													"git pull",
													"echo ",
													"echo git status:",
													"git status",
													"echo enable execution of cgi-bin directory files",
													"cd ~/git-repos/eigenframe/cgi-bin",
													"chmod 755 *.sh",
													"chmod 755 *.rb"
												]
											}
										}
									]
								}
							}
						]
					}
				}
			}
		},

    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "git status",
        "icon": {
          "name": "info.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "git status",
            "component_list": [
              {
                "type": "Clone",
                "name": "LayoutVerticalScrollable",
                "attributes": {
                  "component_list": [
                    {
                      "type": "Clone",
                      "name": "TextViewDetail",
                      "attributes": {
                        "text_size": "10",
                        "ssh_script": "'cd ' + eigenFragment.getViewState(991001) + '; git status'"
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Termux Home (sftp home):"
      }
    },

    {
      "type": "TextView",
      "id": 991000,
      "sftp": "pwd",
      "layout_width": "match_parent",
      "text_color": "#ffffff",
      "text_size": "20",
      "gravity": "left",
      "background_color": "#000000",
      "margin": {
        "left": 10,
        "top": 10,
        "right": 10,
        "bottom": 10
      }
    },

    {
      "type": "Clone",
      "name": "HorizontalLine"
    },

    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "GIT Working Directory: \n(relative to sftp home)"
      }
    },

    {
      "type": "EditText",
      "id": 991001,
      "text": "git-repos/eigenframe",
      "max_length": 200,
      "is_single_line": true,
      "layout_width": "match_parent",
      "text_color": "#ffffff",
      "text_size": "20",
      "gravity": "left",
      "background_color": "#000000",
      "margin": {
        "left": 10,
        "top": 10,
        "right": 10,
        "bottom": 10
      }
    },

    {
      "type": "Clone",
      "name": "HorizontalLine"
    },

    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Current GIT Path:"
      }
    },
    {
      "type": "EditText",
      "id": 991002,
      "text": "",
      "max_length": 200,
      "is_single_line": true,
      "layout_width": "match_parent",
      "text_color": "#ffffff",
      "text_size": "20",
      "gravity": "left",
      "background_color": "#000000",
      "margin": {
        "left": 10,
        "top": 10,
        "right": 10,
        "bottom": 10
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Selected Filename: "
      }
    },
    {
      "type": "EditText",
      "id": 991003,
      "max_length": 200,
      "is_single_line": true,
      "layout_width": "match_parent",
      "text": "web/frames/playlist.json",
      "text_color": "#ffffff",
      "text_size": "20",
      "gravity": "left",
      "background_color": "#000000",
      "margin": {
        "left": 10,
        "top": 10,
        "right": 10,
        "bottom": 10
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Navigation: "
      }
    },
    {
      "type": "ToggleButton",
      "text_on": "Clear Path",
      "text_off": "Reset Path",
      "checked": false,
      "text_color": "#ffffff",
      "background_color": "#654321",
      "gravity": "center",
      "icon": {
        "name": "cancel.",
        "location": "top"
      },
      "margin": {
        "left": 10,
        "top": 10,
        "right": 10,
        "bottom": 10
      },
      "on_click": {
        "type": "JavaScript",
        "script_list": [
          "if (eigenFragment.getViewState() == '1') eigenFragment.setViewState(991001,'git-repos/eigenframe')",
          "else eigenFragment.setViewState(991001,'')",
          "eigenFragment.setViewState(991002,'')",
          "eigenFragment.setViewState(991003,'')"
        ]
      }
    },

    {
      "type": "Button",
      "text": "Explore Current GIT Path",
      "layout_width": "wrap_content",
      "layout_height": "wrap_content",
      "text_color": "#ffffff",
      "background_color": "#654321",
      "icon": {
        "name": "listview.",
        "location": "top"
      },
      "margin": {
        "left": 10,
        "top": 10,
        "right": 10,
        "bottom": 10
      },
      "on_click": {
        "type": "SecureFtp",
        "id": 991004,
        "sftp_script": "'directory ' + eigenFragment.concatPaths(991000, 991001, 991002)",
        "on_complete": {
          "type": "PopupScreen",
          "title_script": "''+ eigenFragment.concatPaths(991000, 991001, 991002)",
          "url_script": "eigenFragment.getCacheString('RootUrl')+'frames/ssh-git-admin-listview.json'",
          "layout_width": "match_parent",
          "text_size": "14",
          "text_color": "#ffffff",
          "background_color": "#111111"
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Check differences between selected file and master: "
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "git diff from master",
        "icon": {
          "name": "check.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "git diff from master",
            "component_list": [
              {
                "type": "Clone",
                "name": "LayoutVerticalScrollable",
                "attributes": {
                  "component_list": [
                    {
                      "type": "Clone",
                      "name": "TextViewDetail",
                      "attributes": {
                        "text_size": "10",
                        "ssh_script": "'cd ' + eigenFragment.getViewState(991001) + '; git diff master:' + eigenFragment.concatPaths(991002, 991003) + ' -- ' + eigenFragment.concatPaths(991002, 991003); "
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "git restore (revert to master)",
        "icon": {
          "name": "undo.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "git restore",
            "component_list": [
              {
                "type": "Clone",
                "name": "LayoutVerticalScrollable",
                "attributes": {
                  "component_list": [
                    {
                      "type": "Clone",
                      "name": "TextViewDetail",
                      "attributes": {
                        "text_size": "10",
                        "ssh_script": "'cd ' + eigenFragment.getViewState(991001) + '; git restore ' + eigenFragment.concatPaths(991002, 991003) "
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },

    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Commit Changes in GIT Working Directory to GITHUB (Test Branch): "
      }
    },

    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "git add *",
        "icon": {
          "name": "add.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "git add *",
            "component_list": [
              {
                "type": "Clone",
                "name": "LayoutVerticalScrollable",
                "attributes": {
                  "component_list": [
                    {
                      "type": "Clone",
                      "name": "TextViewDetail",
                      "attributes": {
                        "text_size": "10",
                        "ssh_script": "'cd ' + eigenFragment.getViewState(991001) + '; git add *'"
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "git commit -a m 'testing'",
        "icon": {
          "name": "save.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "git commit -a m 'testing'",
            "component_list": [
              {
                "type": "Clone",
                "name": "LayoutVerticalScrollable",
                "attributes": {
                  "component_list": [
                    {
                      "type": "Clone",
                      "name": "TextViewDetail",
                      "attributes": {
                        "text_size": "10",
                        "ssh_script": "'cd ' + eigenFragment.getViewState(991001) + '; git commit -a -m \"testing\"'"
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "git push origin test",
        "icon": {
          "name": "cloud_upload.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "git push origin test",
            "component_list": [
              {
                "type": "Clone",
                "name": "LayoutVerticalScrollable",
                "attributes": {
                  "component_list": [
                    {
                      "type": "Clone",
                      "name": "TextViewDetail",
                      "attributes": {
                        "text_size": "10",
                        "ssh_script": "'cd ' + eigenFragment.getViewState(991001) + '; git push origin test' "
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Sign into GITHUB, select Test branch, create pull request, and merge: "
      }
    },
    {
      "type": "HtmlView",
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "gravity": "center",
      "x_text_size": "14",
      "text_color": "#ffffff",
      "html_list": [
        "<img src='ic_launcher.'>",
        "<h1><font color=\"#8aec81\">EigenFrame Github Webpage: </font></h1>",
        "https://github.com/jsawaya/eigenframe"
      ]
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    }
  ]
}


