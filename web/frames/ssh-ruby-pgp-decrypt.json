{
	"comment": "set the HomeDirectory from sftp pwd",
	"type": "SecureFtp",
	"id": 411000,
	"sftp_script": "'pwd'",
	"on_complete": {
		"type": "LinearLayout",
		"scrollable": true,
		"orientation": "vertical",
		"gravity": "center_horizontal",
		"layout_width": "match_parent",
		"component_list": [
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "Clone",
				"name": "TextViewLabel",
				"attributes": {
					"text": "Decrypt File"
				}
			},
			{
				"type": "Clone",
				"name": "TextViewDetail",
				"attributes": {
					"text_list": [
						"OpenPGP Decryption",
						"- your private key enables you to open files that have been encrypted with your public key",
						"- try decrypting a file encrypted with demo-private-key's public key: ",
						"passphrase: demo"
					]
				}
			},
			{
				"type": "JavaScript",
				"script": "eigenFragment.setCacheString('SelectFilePrompt', 0, 'Select Encrypted File')"
			},
			{
				"type": "JavaScript",
				"script_list": [
					"eigenFragment.setCacheString('WorkingDirectory', '')",
					"eigenFragment.setCacheString('SelectedPath', 'git-repos/eigenframe')",
					"eigenFragment.setCacheString('SelectedFile', '')"
				]
			},
			{
				"type": "Clone",
				"name": "JavaScriptSelectFilePath"
			},
			{
				"type": "Clone",
				"name": "EditTextPgpPassword6606"
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "TEST1 (via ssh/ruby)",
					"icon": {
						"name": "open_browser.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "TEST1",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_list": [
														"cd /data/data/com.termux/files/home/git-repos/eigenframe/cgi-bin;",
														"TEST1='p 1;'",
														"TEST1+='p 2;'",
														"TEST1+='p 3;'",
														"echo ruby ssh_ruby.rb \"$TEST1\" "
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Button",
				"text": "Decrypt File",
				"layout_width": "wrap_content",
				"layout_height": "wrap_content",
				"text_color": "#ffffff",
				"background_color": "#654321",
				"icon": {
					"name": "info.",
					"location": "top"
				},
				"on_click": {
					"type": "UrlRequest",
					"id": 6605,
					"is_eigen_response": false,
					"url": "http://localhost:8080/cgi-bin/cgi_ruby.rb",
					"parameter_list": [
						{
							"name": "encrypted_file",
							"value_script_list": [
								"var targetVid = eigenFragment.getIntegerVariable('TargetViewId', 0, 0)",
								"var encrypted_file = eigenFragment.getViewState(targetVid)",
								"'' + encrypted_file"
							]
						},
						{
							"name": "password",
							"value_script_list": [
								"'' + eigenFragment.getViewState(6606)"
							]
						},
						{
							"name": "ruby",
							"value_list": [
								"encrypted_file = cgi.params['encrypted_file'][0]",
								"output_file = encrypted_file[0..(encrypted_file.rindex('.')-1)]",
								"password = cgi.params['password'][0]",
								"gpg_decrypt(password, encrypted_file, output_file).to_json.to_s"
							]
						}
					],
					"on_complete": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "Decrypt File",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"url": "file:///data/user/0/com.sawaya.eigenframe/files/UrlRequest:6605"
												}
											}
										]
									}
								}
							]
						}
					}
				}
			}
		]
	}
}
