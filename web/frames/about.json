{
  "component_list": [
    {
      "gravity": "center",
      "html_list": [
        "<img src='ic_launcher.'>",
        "<h1><font color=\"#8aec81\">EigenFrame: <\/font><\/h1>",
        "<h4><font color=\"#8aec81\">Framework for Dynamic Android Apps<\/font><\/h4>",
        "{Specification, Deployment, Rendering, Execution}"
      ],
      "layout_height": "wrap_content",
      "layout_width": "match_parent",
      "text_color": "#ffffff",
      "type": "HtmlView",
      "x_text_size": "14"
    },
    {
      "gravity": "left",
      "html_script": "'<h4>GitHub README:<\/h4> https://github.com/jsawaya/eigenframe/blob/' + eigenActivity.getGithubBranchString() + '/README.md'",
      "layout_width": "match_parent",
      "text_color": "#ffffff",
      "type": "HtmlView"
    },
    {
      "background_color": "#654321",
      "icon": {
        "location": "top",
        "name": "info."
      },
      "margin": {
        "bottom": 10,
        "left": 10,
        "right": 10,
        "top": 10
      },
      "on_click": {
        "font": "font/droid-sans-mono/DroidSansMono.ttf",
        "text_script": "'Version: '+eigenActivity.packageInfo.versionName",
        "title": "EigenFrame Version",
        "type": "PopupTextView"
      },
      "text": "Show EigenFrame Version",
      "text_color": "#ffffff",
      "type": "Button"
    },
    {
      "background_color": "#654321",
      "icon": {
        "location": "top",
        "name": "info."
      },
      "margin": {
        "bottom": 10,
        "left": 10,
        "right": 10,
        "top": 10
      },
      "on_click": {
        "font": "font/droid-sans-mono/DroidSansMono.ttf",
        "text_script": "'Edition: '+eigenActivity.getEdition()",
        "title": "EigenFrame Edition",
        "type": "PopupTextView"
      },
      "text": "Show EigenFrame Edition",
      "text_color": "#ffffff",
      "type": "Button"
    },
    {
      "background_color": "#654321",
      "icon": {
        "location": "top",
        "name": "info."
      },
      "margin": {
        "bottom": 10,
        "left": 10,
        "right": 10,
        "top": 10
      },
      "on_click": {
        "script": "eigenActivity.dialogEula()",
        "type": "JavaScript"
      },
      "text": "End User License Agreement",
      "text_color": "#ffffff",
      "type": "Button"
    },
    {
      "name": "HorizontalLine",
      "type": "Clone"
    },
    {
      "attributes": {
        "text": "Settings:"
      },
      "name": "TextViewLabel",
      "type": "Clone"
    },
    {
      "background_color": "#654321",
      "checked_script": "eigenFragment.getBooleanVariable('IsSourceButton', 0, true)",
      "icon": {
        "location": "top",
        "name": "pageview."
      },
      "id": 999300,
      "margin": {
        "bottom": 10,
        "left": 10,
        "right": 10,
        "top": 10
      },
      "on_click": {
        "script_list": [
          "var b1 = eigenFragment.getViewState(999300) == 1;",
          "eigenActivity.setBooleanVariable('IsSourceButton', 0, b1);",
          "eigenActivity.finish();",
          "eigenActivity.launch(eigenActivity.appUrlString);"
        ],
        "type": "JavaScript"
      },
      "padding": {
        "bottom": 30,
        "left": 30,
        "right": 30,
        "top": 30
      },
      "text": "Enable Source Button",
      "text_color": "#ffffff",
      "type": "Switch"
    },
    {
      "attributes": {
        "icon": {
          "location": "top",
          "name": "rotate_90."
        },
        "on_click": {
          "attributes": {
            "title": "Screen Orientation",
            "x-url": "https://raw.githubusercontent.com/jsawaya/eigenframe/master/web/frames/orientation-settings.json",
            "url_script_list": [
              "var github_branch = eigenActivity.getGithubBranchString();",
              "'https://raw.githubusercontent.com/jsawaya/eigenframe/' + github_branch + '/web/frames/orientation-settings.json'"
            ]
          },
          "name": "PopupScreenDefault",
          "type": "Clone"
        },
        "text": "Screen Orientation"
      },
      "name": "ButtonDefault",
      "type": "Clone"
    },
    {
      "name": "HorizontalLine",
      "type": "Clone"
    }
  ],
  "gravity": "center_horizontal",
  "layout_height": "wrap_content",
  "layout_width": "match_parent",
  "orientation": "vertical",
  "scrollable": "true",
  "type": "LinearLayout"
}