{
  "type": "LinearLayout",
  "scrollable": true,
  "orientation": "vertical",
  "layout_width": "match_parent",
  "layout_height": "wrap_content",
  "component_list": [
    {
      "type": "TextView",
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text": "Test ToggleButton",
      "text_size": "20",
      "gravity": "CENTER_HORIZONTAL, CENTER_VERTICAL",
      "text_color": "#ffffff",
      "icon": {
        "name": "sawaya_eigenframe_icon.png",
        "location": "top"
      }
    },
    {
      "type": "HorizontalLine",
      "size": 1,
      "color": "#00ff00"
    },
    {
      "type": "ToggleButton",
      "id": 400,
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "checked": false,
      "text_color": "#ffffff",
      "background_color": "#ff00ff",
      "margin-x": {
        "left": 40,
        "top": 40,
        "right": 40,
        "bottom": 40
      },
      "on_click": {
        "type": "JavaScript",
        "script_list": [
          "eigenActivity.showToast('ToggleButton getViewState: '+eigenFragment.getViewIdState(400))"
        ]
      }
    },
    {
      "type": "Button",
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text": "getViewIdState ToggleButton:400",
      "text_color": "#ffffff",
      "background_color": "#ff00ff",
      "on_click": {
        "type": "JavaScript",
        "script_list": [
          "eigenActivity.showToast('ToggleButton getViewState: '+eigenFragment.getViewIdState(400))"
        ]
      }
    },
    {
      "type": "ToggleButton",
      "id": 401,
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "checked": false,
      "text_color": "#ffffff",
      "background_color": "#ff00ff",
      "margin": {
        "left": 40,
        "top": 40,
        "right": 40,
        "bottom": 40
      }
    },
    {
      "type": "Button",
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text": "getViewIdState ToggleButton:401",
      "text_color": "#ffffff",
      "background_color": "#ff00ff",
      "on_click": {
        "type": "JavaScript",
        "script_list": [
          "eigenActivity.showToast('ToggleButton getViewState: '+eigenFragment.getViewIdState(401))"
        ]
      }
    }
  ]
}
