{
  "type": "LinearLayout",
  "scrollable": true,
  "orientation": "vertical",
  "gravity": "center_horizontal",
  "layout_width": "match_parent",
  "layout_height": "wrap_content",
  "component_list": [
    {
      "type": "TextView",
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text": "Test ToggleButton",
      "text_size": "14",
      "gravity": "CENTER_HORIZONTAL, CENTER_VERTICAL",
      "text_color": "#ffffff",
      "icon": {
        "name": "ic_launcher_round.",
        "location": "top"
      }
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "ToggleButton",
      "id": 400,
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "checked": false,
      "text_color": "#ffffff",
      "background_color": "#ff00ff",
      "margin-x": {
        "left": 40,
        "top": 40,
        "right": 40,
        "bottom": 40
      },
      "on_click": {
        "type": "JavaScript",
        "script_list": [
          "eigenActivity.showToast('ToggleButton getViewState: '+eigenFragment.getViewState(400))"
        ]
      }
    },
    {
      "type": "Button",
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text": "getViewState ToggleButton:400",
      "text_color": "#ffffff",
      "background_color": "#ff00ff",
      "on_click": {
        "type": "JavaScript",
        "script_list": [
          "eigenActivity.showToast('ToggleButton getViewState: '+eigenFragment.getViewState(400))"
        ]
      }
    },
    {
      "type": "ToggleButton",
      "id": 409,
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "checked": false,
      "text_color": "#ffffff",
      "background_color": "#ff00ff",
      "margin": {
        "left": 40,
        "top": 40,
        "right": 40,
        "bottom": 40
      }
    },
    {
      "type": "Button",
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text": "getViewState ToggleButton:409",
      "text_color": "#ffffff",
      "background_color": "#ff00ff",
      "on_click": {
        "type": "JavaScript",
        "script_list": [
          "eigenActivity.showToast('ToggleButton getViewState: '+eigenFragment.getViewState(409))"
        ]
      }
    },
    {
      "type": "ToggleButton",
      "id": 408,
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "checked": true,
      "text_on": "Master",
      "text_off": "Beta",
      "text_color": "#ffffff",
      "background_color": "#654321",
      "margin-x": {
        "left": 40,
        "top": 40,
        "right": 40,
        "bottom": 40
      },
      "on_click": {
        "type": "JavaScript",
        "script_list": [
          "eigenActivity.showToast('ToggleButton getViewState: '+eigenFragment.getViewState(408))"
        ]
      }
    }
  ]
}
