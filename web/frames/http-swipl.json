						"script_list": [
							"var position = eigenFragment.getOptionPosition()",
							"var url = 'http://localhost:8000/'",
							"if (position == 0) url += 'frame?file=test-EditText.json' ",
							"if (position == 1) url += 'parse?file=test-EditText.json' ",
							"if (position == 2) url += 'parse?file=test-Button.json' ",
							"if (position == 3) url += 'search?type=EditText' ",
							"if (position == 4) url += 'search?type=Switch' ",
							"if (position == 5) url += 'search?type=ListView' ",
							"if (position == 6) url += 'search?type=SelectDialog' ",
							"if (position == 7) url += 'search?type=JavaScript' ",
							"if (position == 8) url += 'search?type=UrlRequest' ",
							"if (position == 9) url += 'search?type=SecureShell' ",
							"if (position ==10) url += 'proxy?url=https://raw.githubusercontent.com/jsawaya/eigenframe/1.3/web/apps/app_github_master.json' ",
							"if (position ==11) url += 'proxy?url=https://raw.githubusercontent.com/jsawaya/eigenframe/1.3/web/frames/playlist.json' ",
							"if (position ==12) url += 'prolog?dbug=true&g=nominal_TextView(D,\\'test\\',14),show_json(D)' ",
							"if (position ==13) url += 'prolog?mime=json&g=nominal_TextView(D,\\'test\\',14),show_json(D)' ",
							"eigenFragment.setViewState(999112, url)"
						],
						"type": "JavaScript"
					},
					"option_list": [
						" 0. show file: test-EditText.json",
						" 1. parse file: test-EditText.json",
						" 2. parse file: test-Button.json",
						" 3. search for type: EditText",
