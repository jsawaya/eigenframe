{
  "type": "LinearLayout",
  "scrollable": true,
  "orientation": "vertical",
  "gravity": "center_horizontal",
  "layout_width": "match_parent",
  "layout_height": "match_parent",
  "component_list": [
		{
			"type": "Clone",
			"name": "TextViewLabel",
			"attributes": {
				"text": "EigenFrame Documentation:"
			}
		},
		{
			"type": "Button",
			"layout_width": "match_parent",
