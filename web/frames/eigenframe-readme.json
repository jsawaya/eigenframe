			"background_color": "#654321",
			"icon": {
				"name": "info.",
				"location": "top"
			},
			"margin": {
				"left": 10,
				"top": 10,
				"right": 10,
				"bottom": 10
			},
			"on_click": {
				"type": "PopupScreen",
				"layout_width": "match_parent",
				"layout_height": "wrap_content",
				"text_size": "14",
				"text_color": "#ffffff",
				"background_color": "#111111",
				"title": "README",
				"icon": {
					"name": "info.",
					"location": "left"
				},
