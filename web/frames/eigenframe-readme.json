{
  "type": "LinearLayout",
  "scrollable": true,
  "orientation": "vertical",
  "gravity": "center_horizontal",
  "layout_width": "match_parent",
  "layout_height": "match_parent",
  "component_list": [
    {
      "type": "TextView",
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text_size": "14",
      "text_color": "#ffffff",
			"text": "EigenFrame Documentation"
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
				"text": "GitHub: jsawaya/eigenframe/README.md",
        "icon": {
          "name": "settings.",
          "location": "top"
        },
        "on_complete": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
						"title": "GitHub: jsawaya/eigenframe/README.md",
            "component_list": [
              {
								"type": "WebView",
								"layout_width": "match_parent",
								"layout_height": "wrap_content",
								"is_javascript_enabled": true,
								"url": "'https://github.com/jsawaya/eigenframe/blob/beta/README.md'"
              }
            ]
          }
        }
      }
    }
	]
}
