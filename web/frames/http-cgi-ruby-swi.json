{
	"type": "LinearLayout",
	"scrollable": true,
	"orientation": "vertical",
	"gravity": "center_horizontal",
	"layout_width": "match_parent",
	"component_list": [
		{
			"type": "Clone",
			"name": "HorizontalLine"
		},
		{
			"type": "Clone",
			"name": "TextViewLabel",
			"attributes": {
				"text": "Prolog Applications"
			}
		},
		{
			"type": "Clone",
			"name": "TextViewDetail",
			"attributes": {
				"text_list": [
					"Prolog (Programming in Logic) a language based on predicate calculus to evaluate if a certain proposition can be inferred from a knowledge base (rules) using backward chaining algorithms. "
				]
			}
		},
		{
			"type": "Clone",
			"name": "HorizontalLine"
		},
		{
			"type": "Clone",
			"name": "ButtonDefault",
			"attributes": {
				"text": "Prolog Example",
				"icon": {
					"name": "open_browser.",
					"location": "top"
				},
				"on_click": {
					"type": "Clone",
					"name": "PopupScreenDefault",
					"attributes": {
						"title": "Prolog Example",
						"component_list": [
							{
								"type": "Clone",
								"name": "LayoutVerticalScrollable",
								"attributes": {
									"component_list": [
										{
											"type": "Clone",
											"name": "TextViewDetail",
											"attributes": {
												"text_size": "10",
												"ssh_list": [
													"cd /data/data/com.termux/files/home/git-repos/eigenframe/cgi-bin",
													"ruby ssh_ruby.rb 'swipl_test(\"swipl-lib/routes.pl\", \"route(nottinghill_gate,maida_vale,X)\")' "
												]
											}
										}
									]
								}
							}
						]
					}
				}
			}
		},
		{
			"type": "Clone",
			"name": "HorizontalLine"
		},
		{
			"type": "JavaScript",
			"script": "eigenFragment.setCacheString('SelectFilePrompt', 0, 'Select Prolog File (KB)')"
		},
		{
			"type": "Clone",
			"name": "JavaScriptSelectFilePath"
		},
		{
			"type": "Clone",
			"name": "LayoutVerticalScrollable",
			"attributes": {
				"component_list": [
					{
						"type": "Clone",
						"name": "ButtonDefault",
						"attributes": {
							"text": "Show Prolog File",
							"icon": {
								"name": "open_browser.",
								"location": "top"
							},
							"on_click": {
								"type": "Clone",
								"name": "PopupScreenShowRemoteFile"
							}
						}
					},
					{
						"type": "Clone",
						"name": "ButtonDefault",
						"attributes": {
							"text": "Edit Prolog File",
							"icon": {
								"name": "open_browser.",
								"location": "top"
							},
							"on_click": {
								"type": "Clone",
								"name": "PopupScreenEditRemoteFile"
							}
						}
					}
				]
			}
		},

		{
			"type": "EditText",
			"id": 7602,
			"layout_width": "match_parent",
			"layout_height": "wrap_content",
			"text": "route(nottinghill_gate,maida_vale,X)",
			"text_color": "#ffffff",
			"background_color": "#11ff0000"
		},
		{
			"type": "Clone",
			"name": "ButtonDefault",
			"attributes": {
				"text": "Prolog Example2",
				"icon": {
					"name": "open_browser.",
					"location": "top"
				},
				"on_click": {
					"type": "Clone",
					"name": "PopupScreenDefault",
					"attributes": {
						"title": "Prolog Example2",
						"component_list": [
							{
								"type": "Clone",
								"name": "LayoutVerticalScrollable",
								"attributes": {
									"component_list": [
										{
											"type": "Clone",
											"name": "TextViewDetail",
											"attributes": {
												"text_size": "10",
												"ssh_script_list": [
													"var dir = '/data/data/com.termux/files/home/git-repos/eigenframe/cgi-bin' ",
													"swipl_path = eigenFragment.getCacheString('FullPath') ",
													"swipl_goal = eigenFragment.getViewState(7602) ",
													"'cd '+dir+'; ruby ssh_swipl.rb \"'+swipl_path+'\" \"'+swipl_goal+'\"  '"
												]
											}
										}
									]
								}
							}
						]
					}
				}
			}
		},
		{
			"type": "Clone",
			"name": "HorizontalLine"
		},
		{
			"type": "Clone",
			"name": "TextViewDetail",
			"attributes": {
				"text_list": [
					"TO DO: more examples"
				]
			}
		}
	]
}

