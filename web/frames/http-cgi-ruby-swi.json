{
	"type": "LinearLayout",
	"scrollable": true,
	"orientation": "vertical",
	"gravity": "center_horizontal",
	"layout_width": "match_parent",
	"component_list": [
		{
			"type": "Clone",
			"name": "HorizontalLine"
		},
		{
			"type": "Clone",
			"name": "TextViewLabel",
			"attributes": {
				"text": "Prolog Applications"
			}
		},
		{
			"type": "Clone",
			"name": "TextViewDetail",
			"attributes": {
				"text_list": [
					"Prolog (Programming in Logic) a language based on predicate calculus to evaluate if a certain proposition can be inferred from a knowledge base (rules) using backward chaining algorithms. "
				]
			}
		},
		{
			"type": "Clone",
			"name": "HorizontalLine"
		},
		{
			"type": "Clone",
			"name": "ButtonDefault",
			"attributes": {
				"text": "Prolog Example",
				"icon": {
					"name": "open_browser.",
					"location": "top"
				},
				"on_click": {
					"type": "Clone",
					"name": "PopupScreenDefault",
					"attributes": {
						"title": "Prolog Example",
						"component_list": [
							{
								"type": "Clone",
								"name": "LayoutVerticalScrollable",
								"attributes": {
									"component_list": [
										{
											"type": "x-SecureShell",
											"is_eigen_response": false,
											"ssh_list": [
												"cd /data/data/com.termux/files/home/git-repos/eigenframe/cgi-bin",
												"ruby ssh-directory-listview.rb /data/data/com.termux/files/home"
											]
										},
										{
											"type": "Clone",
											"name": "TextViewDetail",
											"attributes": {
												"text_size": "10",
												"ssh_list": [
													"cd /data/data/com.termux/files/home/git-repos/eigenframe/cgi-bin",
													"ruby ssh_ruby.rb 'swipl_test(\"swipl-lib/routes.pl\", \"route(nottinghill_gate,maida_vale,X)\")' "
												]
											}
										}
									]
								}
							}
						]
					}
				}
			}
		},
		{
			"type": "EditText",
			"id": 7601,
			"layout_width": "match_parent",
			"layout_height": "wrap_content",
			"text": "swipl-lib/routes.pl",
			"text_color": "#ffffff",
			"background_color": "#11ff0000"
		},
		{
			"type": "EditText",
			"id": 7602,
			"layout_width": "match_parent",
			"layout_height": "wrap_content",
			"text": "route(nottinghill_gate,maida_vale,X)",
			"text_color": "#ffffff",
			"background_color": "#11ff0000"
		},
		{
			"type": "Clone",
			"name": "ButtonDefault",
			"attributes": {
				"text": "Prolog Example2",
				"icon": {
					"name": "open_browser.",
					"location": "top"
				},
				"on_click": {
					"type": "Clone",
					"name": "PopupScreenDefault",
					"attributes": {
						"title": "Prolog Example2",
						"component_list": [
							{
								"type": "Clone",
								"name": "LayoutVerticalScrollable",
								"attributes": {
									"component_list": [
										{
											"type": "Clone",
											"name": "TextViewDetail",
											"attributes": {
												"text_size": "10",
												"ssh_script_list": [
													"var dir = '/data/data/com.termux/files/home/git-repos/eigenframe/cgi-bin' ",
													"swipl_path0 = 'swipl-lib/routes.pl' ",
													"swipl_goal0 = 'route(nottinghill_gate,maida_vale,X)' ",
													"swipl_path = eigenFragment.getViewState(7601) ",
													"swipl_goal = +eigenFragment.getViewState(7602) ",
													"'cd '+dir+'; ruby ssh_swipl.rb \"'+swipl_path+'\" \"'+swipl_goal+'\"  '"
												]
											}
										}
									]
								}
							}
						]
					}
				}
			}
		},
		{
			"type": "Clone",
			"name": "HorizontalLine"
		},
		{
			"type": "Clone",
			"name": "TextViewDetail",
			"attributes": {
				"text_list": [
					"TO DO: more examples"
				]
			}
		}
	]
