{
  "type": "LinearLayout",
  "scrollable": true,
  "orientation": "vertical",
  "gravity": "center_horizontal",
  "layout_width": "match_parent",
  "component_list": [

    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Install GIT package"
      }
    },

    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_list": [
          "Click [Install git package]  ..."
        ]
      }
    },

    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Install git package",
        "icon": {
          "name": "file_download.",
          "location": "top"
        },
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "Install git package",
            "component_list": [
              {
                "type": "Clone",
                "name": "LayoutVerticalScrollable",
                "attributes": {
                  "component_list": [
                    {
                      "type": "Clone",
                      "name": "TextViewDetail",
                      "attributes": {
                        "text_size": "10",
                        "ssh_list": [
                          "echo pkg install git",
                          "pkg install git"
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },


    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_list": [
          "Click [Make /git-repos directory], prepare to download local eigenframe git working directory"
        ]
      }
    },

    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Make git-repos directory",
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "Make /git-repos directory",
            "component_list": [
              {
                "type": "Clone",
                "name": "LayoutVerticalScrollable",
                "attributes": {
                  "component_list": [
                    {
                      "type": "Clone",
                      "name": "TextViewDetail",
                      "attributes": {
                        "text_size": "10",
                        "ssh_list": [
                          "cd ~",
                          "mkdir git-repos",
                          "cd git-repos",
                          "echo pwd:",
                          "pwd"
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },

    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_list": [
          "Click [Clone eigenframe github repository] to download eigenframe from github, and checkout a test branch..."
        ]
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "Clone eigenframe from github\nand checkout test branch",
        "on_click": {
          "type": "Clone",
          "name": "PopupScreenDefault",
          "attributes": {
            "title": "Clone eigenframe...",
            "component_list": [
              {
                "type": "Clone",
                "name": "LayoutVerticalScrollable",
                "attributes": {
                  "component_list": [
                    {
                      "type": "Clone",
                      "name": "TextViewDetail",
                      "attributes": {
                        "text_size": "10",
                        "ssh_list": [
                          "cd ~/git-repos",
                          "echo git clone https://github.com/jsawaya/eigenframe.git",
                          "git clone https://github.com/jsawaya/eigenframe.git",
                          "echo ",
                          "echo cd eigenframe",
                          "cd eigenframe",
                          "echo ",
                          "echo git checkout -b test",
                          "git checkout -b test",
                          "echo ",
                          "echo git status:",
                          "git status",
                          "echo git remote -v:",
                          "git remote -v"
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "type": "Clone",
      "name": "TextViewDetail",
      "attributes": {
        "text_list": [
          "Initial github setup in Termux:",
          " cd git-repos/eigenframe",
          " git config credential.helper store",
          " git push origin http://github.com/jsawaya/eigenframe.git",
          " git push origin test"
        ]
      }
    }


  ]
}



