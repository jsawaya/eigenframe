{
  "comment": "set the HomeDirectory from sftp pwd",
  "type": "SecureFtp",
  "id": 411000,
  "sftp": "pwd",
  "on_complete": {
    "type": "LinearLayout",
    "scrollable": true,
    "orientation": "vertical",
    "gravity": "center_horizontal",
    "layout_width": "match_parent",
    "component_list": [
      {
        "type": "Clone",
        "name": "TextViewLabel",
        "attributes": {
          "text": "GIT Installation"
        }
      },
      {
        "type": "Clone",
        "name": "HorizontalLine"
      },
      {
        "type": "Clone",
        "name": "TextViewDetail",
        "attributes": {
          "text_list": [
            "Click [Install git package] to install git, then download git-repos/eigenframe"
          ]
        }
      },
      {
        "type": "Clone",
        "name": "ButtonDefault",
        "attributes": {
          "text": "Install git package",
          "icon": {
            "name": "file_download.",
            "location": "top"
          },
          "on_click": {
            "type": "Clone",
            "name": "PopupScreenDefault",
            "attributes": {
              "title": "Install git package",
              "component_list": [
                {
                  "type": "Clone",
                  "name": "LayoutVerticalScrollable",
                  "attributes": {
                    "component_list": [
                      {
                        "type": "Clone",
                        "name": "TextViewDetail",
                        "attributes": {
                          "text_size": "10",
                          "is_toast": true,
                          "ssh_list": [
                            "echo pkg install git",
                            "echo y | pkg install git"
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        }
      },
      {
        "type": "Clone",
        "name": "HorizontalLine"
      },
      {
        "type": "Clone",
        "name": "TextViewDetail",
        "attributes": {
          "text_list": [
            "Click [Make /git-repos directory], prepare to download local eigenframe git working directory"
          ]
        }
      },
      {
        "type": "Clone",
        "name": "ButtonDefault",
        "attributes": {
          "text": "Make git-repos directory",
          "on_click": {
            "type": "Clone",
            "name": "PopupScreenDefault",
            "attributes": {
              "title": "Make /git-repos directory",
              "component_list": [
                {
                  "type": "Clone",
                  "name": "LayoutVerticalScrollable",
                  "attributes": {
                    "component_list": [
                      {
                        "type": "Clone",
                        "name": "TextViewDetail",
                        "attributes": {
                          "text_size": "10",
                          "ssh_list": [
                            "cd ~",
                            "mkdir git-repos",
                            "cd git-repos",
                            "echo pwd:",
                            "pwd"
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        }
      },
      {
        "type": "Clone",
        "name": "HorizontalLine"
      },
      {
        "type": "Clone",
        "name": "TextViewDetail",
        "attributes": {
          "text_list": [
            "Click [Clone eigenframe github repository] to download eigenframe from github, and checkout a test branch..."
          ]
        }
      },
      {
        "type": "Clone",
        "name": "ButtonDefault",
        "attributes": {
          "text": "Clone eigenframe from github",
          "on_click": {
            "type": "Clone",
            "name": "PopupScreenDefault",
            "attributes": {
              "title": "Clone eigenframe...",
              "component_list": [
                {
                  "type": "Clone",
                  "name": "LayoutVerticalScrollable",
                  "attributes": {
                    "component_list": [
                      {
                        "type": "Clone",
                        "name": "TextViewDetail",
                        "attributes": {
                          "text_size": "10",
                          "is_toast": true,
                          "ssh_list": [
                            "cd ~/git-repos",
                            "echo //git clone https://github.com/jsawaya/eigenframe.git",
                            "git clone https://github.com/jsawaya/eigenframe.git",
                            "echo //cd eigenframe",
                            "cd eigenframe",
                            "echo //git status",
                            "git status"
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        }
      },
      {
        "type": "Clone",
        "name": "ButtonDefault",
        "attributes": {
          "text": "Checkout (create) local beta branch",
          "on_click": {
            "type": "Clone",
            "name": "PopupScreenDefault",
            "attributes": {
              "title": "Checkout (create) local beta branch",
              "component_list": [
                {
                  "type": "Clone",
                  "name": "LayoutVerticalScrollable",
                  "attributes": {
                    "component_list": [
                      {
                        "type": "Clone",
                        "name": "TextViewDetail",
                        "attributes": {
                          "text_size": "10",
                          "is_toast": true,
                          "ssh_list": [
                            "cd ~/git-repos/eigenframe",
                            "echo //git checkout -b beta",
                            "git checkout -b beta",
                            "echo //git status",
                            "git status"
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        }
      },
      {
        "type": "Clone",
        "name": "TextViewDetail",
        "attributes": {
          "text_list": [
            "Initial github setup in Termux:",
            " cd git-repos/eigenframe",
            " git config credential.helper store",
            " git push origin http://github.com/jsawaya/eigenframe.git",
            " git push origin beta"
          ]
        }
      }
    ]
  }
}