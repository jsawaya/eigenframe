{
  "comment": "set the HomeDirectory from sftp pwd",
  "type": "SecureFtp",
  "id": 411000,
  "sftp": "pwd",
  "on_complete": {
    "type": "LinearLayout",
    "scrollable": true,
    "orientation": "vertical",
    "gravity": "center_horizontal",
    "layout_width": "match_parent",
    "component_list": [
      {
        "type": "Clone",
        "name": "TextViewLabel",
        "attributes": {
          "text": "GIT Installation"
        }
      },
      {
        "type": "Clone",
        "name": "HorizontalLine"
      },
      {
        "type": "Clone",
        "name": "TextViewDetail",
        "attributes": {
          "text_list": [
            "Click [Install git package] to install git, then download git-repos/eigenframe"
          ]
        }
      },
      {
        "type": "Clone",
        "name": "ButtonDefault",
        "attributes": {
          "text": "Install git package",
          "icon": {
            "name": "file_download.",
            "location": "top"
          },
          "on_click": {
            "type": "Clone",
            "name": "PopupScreenDefault",
            "attributes": {
              "title": "Install git package",
              "component_list": [
                {
                  "type": "Clone",
                  "name": "LayoutVerticalScrollable",
                  "attributes": {
                    "component_list": [
                      {
                        "type": "Clone",
                        "name": "TextViewDetail",
                        "attributes": {
                          "text_size": "10",
                          "is_toast": true,
                          "ssh_list": [
                            "echo pkg install git",
                            "echo y | pkg install git"
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        }
      },
      {
        "type": "Clone",
        "name": "HorizontalLine"
      },
      {
        "type": "Clone",
        "name": "TextViewDetail",
        "attributes": {
          "text_list": [
            "Click [Make git-repos directory] prepares a local eigenframe git working directory"
          ]
        }
      },
      {
        "type": "Clone",
        "name": "ButtonDefault",
        "attributes": {
          "text": "Make git-repos directory",
          "on_click": {
            "type": "Clone",
            "name": "PopupScreenDefault",
            "attributes": {
              "title": "Make /git-repos directory",
              "component_list": [
                {
                  "type": "Clone",
                  "name": "LayoutVerticalScrollable",
                  "attributes": {
                    "component_list": [
                      {
                        "type": "Clone",
                        "name": "TextViewDetail",
                        "attributes": {
                          "text_size": "10",
                          "ssh_list": [
                            "cd ~",
                            "mkdir git-repos",
                            "cd git-repos",
                            "echo pwd:",
                            "pwd"
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        }
      },
      {
        "type": "Clone",
        "name": "ButtonDefault",
        "attributes": {
          "text": "Remove git-repos/eigenframe directory\n(if it already exists)",
          "on_click": {
            "type": "Clone",
            "name": "PopupScreenDefault",
            "attributes": {
              "title": "Clear /git-repos directory",
              "component_list": [
                {
                  "type": "Clone",
                  "name": "LayoutVerticalScrollable",
                  "attributes": {
                    "component_list": [
                      {
                        "type": "Clone",
                        "name": "TextViewDetail",
                        "attributes": {
                          "text_size": "10",
                          "ssh_list": [
                            "echo //rm -rf ~/git-repos/eigenframe",
                            "rm -rf ~/git-repos/eigenframe"
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        }
      },
      {
        "type": "Clone",
        "name": "HorizontalLine"
      },
      {
        "type": "Clone",
        "name": "TextViewDetail",
        "attributes": {
          "text_list": [
            "Click [Clone eigenframe] to download EigenFrame repository from GitHub"
          ]
        }
      },
      {
        "type": "Clone",
        "name": "ButtonDefault",
        "attributes": {
          "text": "Clone eigenframe",
          "on_click": {
            "type": "Clone",
            "name": "PopupScreenDefault",
            "attributes": {
              "title": "Clone eigenframe",
              "component_list": [
                {
                  "type": "Clone",
                  "name": "LayoutVerticalScrollable",
                  "attributes": {
                    "component_list": [
                      {
                        "type": "Clone",
                        "name": "TextViewDetail",
                        "attributes": {
                          "text_size": "10",
                          "is_toast": true,
                          "ssh_list": [
                            "cd ~/git-repos",
                            "echo //git clone https://github.com/jsawaya/eigenframe.git",
                            "git clone https://github.com/jsawaya/eigenframe.git",
                            "echo //cd eigenframe",
                            "cd eigenframe",
                            "echo //git status",
                            "git status"
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        }
      },
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
			{
				"type": "Clone",
				"name": "TextViewLabel",
				"attributes": {
					"text": "Select Branch from EigenFrame GitHub"
				}
			},
			{
				"type": "Clone",
				"name": "TextViewDetail",
				"attributes": {
					"text_list": [
						"Click [Checkout Beta Branch] to activate your local beta branch - so you can pull beta updates from EigenFrame GitHub"
					]
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "Checkout Beta Branch",
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "Checkout Beta Branch",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_list": [
														"cd ~/git-repos/eigenframe",
														"echo //git checkout -B beta",
														"git checkout -B beta",
														"echo //git status",
														"git status"
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "Checkout Master Branch",
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "Checkout Master Branch",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_list": [
														"cd ~/git-repos/eigenframe",
														"echo //git checkout master",
														"git checkout master",
														"echo //git status",
														"git status"
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "TextViewDetail",
				"attributes": {
					"text_list": [
						"Click [Fetch/Reset GitHub EigenFrame Beta Branch] to do a hard reset - do this only if your local repository is really a mess"
					]
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "Fetch/Reset EigenFrame GitHub Beta Branch",
					"icon": {
						"name": "cloud_download.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "Fetch/Reset EigenFrame GitHub Beta Branch",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_list": [
														"cd ~/git-repos/eigenframe",
														"echo //git status",
														"git status",
														"echo //git stash",
														"git stash",
														"echo //git fetch origin",
														"git fetch origin",
														"echo //git reset --hard origin/beta",
														"git reset --hard origin/beta",
														"cd ~/git-repos/eigenframe/cgi-bin",
														"chmod 755 *.sh",
														"chmod 755 *.rb",
														"echo //git status",
														"git status"
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "TextViewDetail",
				"attributes": {
					"text_list": [
						"Click [Pull from EigenFrame GitHub] to pull updates for current branch (master/beta) - this is the typical way to get the recent updates from EigenFrame GitHub"
					]
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "Pull from EigenFrame GitHub",
					"icon": {
						"name": "cloud_download.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "Pull from EigenFrame GitHub",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh_list": [
														"cd ~/git-repos/eigenframe",
														"echo //git stash",
														"git stash",
														"echo //git pull",
														"git pull",
														"cd ~/git-repos/eigenframe/cgi-bin",
														"chmod 755 *.sh",
														"chmod 755 *.rb",
														"git status"
													]
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "ButtonDefault",
				"attributes": {
					"text": "git status",
					"icon": {
						"name": "info.",
						"location": "top"
					},
					"on_click": {
						"type": "Clone",
						"name": "PopupScreenDefault",
						"attributes": {
							"title": "git status",
							"component_list": [
								{
									"type": "Clone",
									"name": "LayoutVerticalScrollable",
									"attributes": {
										"component_list": [
											{
												"type": "Clone",
												"name": "TextViewDetail",
												"attributes": {
													"text_size": "10",
													"ssh": "cd ~/git-repos/eigenframe ; git status"
												}
											}
										]
									}
								}
							]
						}
					}
				}
			},
			{
				"type": "Clone",
				"name": "HorizontalLine"
			},
      {
        "type": "Clone",
        "name": "TextViewDetail",
        "attributes": {
          "text_list": [
            "Initial github setup in Termux:",
            " cd git-repos/eigenframe",
            " git config credential.helper store",
            " git push origin https://github.com/jsawaya/eigenframe.git",
            " git push origin beta"
          ]
        }
      }
    ]
  }
}