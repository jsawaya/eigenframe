{
  "type": "LinearLayout",
  "scrollable": true,
  "orientation": "vertical",
  "layout_width": "match_parent",
  "layout_height": "wrap_content",
  "component_list": [
    {
      "type": "TextView",
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text_size": "20",
      "text_color": "#ffffff",
      "text": "Test ImageView (url -> png)"
    },
    {
      "type": "HorizontalLine",
      "size": 2,
      "color": "#00ff00"
    },
    {
      "type": "ImageView",
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "on_click": {
        "type": "JavaScript",
        "script": "eigenActivity.showToast('image click default')"
      }
    },
    {
      "type": "HorizontalLine",
      "size": 2,
      "color": "#00ff00"
    },
    {
      "type": "ImageView",
      "url": "file:///storage/emulated/0/Android/data/com.sawaya.eigenframe.full/files/image/eigenframe.png",
      "layout_width": "match_parent",
      "layout_height": "100",
      "on_click": {
        "type": "JavaScript",
        "script": "eigenActivity.showToast('image click eigenframe')"
      }
    },
    {
      "type": "HorizontalLine",
      "size": 2,
      "color": "#00ff00"
    },
    {
      "type": "ImageView",
      "url": "file:///storage/emulated/0/Android/data/com.sawaya.eigenframe.full/files/image/photopgp.png",
      "layout_width": "match_parent",
      "layout_height": "100",
      "on_click": {
        "type": "JavaScript",
        "script": "eigenActivity.showToast('image click photopgp')"
      }
    },
    {
      "type": "HorizontalLine",
      "size": 2,
      "color": "#00ff00"
    },
    {
      "type": "ImageView",
      "url": "https://raw.githubusercontent.com/jsawaya/eigenframe/master/web/res/photopgp.png",
      "layout_width": "match_parent",
      "layout_height": "100",
      "on_click": {
        "type": "JavaScript",
        "script": "eigenActivity.showToast('image click photopgp')"
      }
    },
    {
      "type": "HorizontalLine",
      "size": 2,
      "color": "#00ff00"
    }
  ]
}
