{
  "type": "LinearLayout",
  "scrollable": true,
  "orientation": "vertical",
  "layout-width": "match_parent",
  "layout-height": "wrap_content",
  "component-list": [
    {
      "type": "TextView",
      "layout-width": "match_parent",
      "layout-height": "wrap_content",
      "text-size": "20",
      "text-color": "#ffffff",
      "text": "Test ImageView (url -> png)"
    },
    {
      "type": "Button",
      "layout-width": "wrap_content",
      "layout-height": "wrap_content",
      "text": "Source",
      "text-color": "#ffffff",
      "background-color": "#22ff0000",
      "icon": {
        "name": "sawaya_eigenframe_icon.png",
        "location": "left"
      },
      "eigen-script": {
        "type": "PopupTextView",
        "title": "Source",
        "url": "https://raw.githubusercontent.com/jsawaya/eigenframe/master/web/frames/test-ImageView.json"
      }
    },
    {
      "type": "HorizontalLine",
      "size": 2,
      "color": "#00ff00"
    },
    {
      "type": "ImageView",
      "layout-width": "match_parent",
      "layout-height": "wrap_content",
      "eigen-script": {
        "type": "JavaScript",
        "script": "eigenActivity.showToast('image click default')"
      }
    },
    {
      "type": "HorizontalLine",
      "size": 2,
      "color": "#00ff00"
    },
    {
      "type": "ImageView",
      "url": "https://raw.githubusercontent.com/jsawaya/eigenframe/master/web/res/photopgp.png",
      "layout-width": "match_parent",
      "layout-height": "100",
      "eigen-script": {
        "type": "JavaScript",
        "script": "eigenActivity.showToast('image click photopgp')"
      }
    },
    {
      "type": "HorizontalLine",
      "size": 2,
      "color": "#00ff00"
    },
    {
      "type": "ImageView",
      "url": "https://raw.githubusercontent.com/jsawaya/eigenframe/master/web/res/eigenframe.png",
      "layout-width": "match_parent",
      "layout-height": "100",
      "eigen-script": {
        "type": "JavaScript",
        "script": "eigenActivity.showToast('image click eigenframe')"
      }
    },
    {
      "type": "HorizontalLine",
      "size": 2,
      "color": "#00ff00"
    }
  ]
}
