{
  "type": "LinearLayout",
  "scrollable": true,
  "orientation": "vertical",
  "gravity": "center_horizontal",
  "layout_width": "match_parent",
  "layout_height": "wrap_content",
  "component_list": [
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "SSH User-id:"
      }
    },

    {
      "type": "EditText",
      "id": 88800,
      "max_length": 40,
      "filter": "no-whitespace",
      "is_single_line": true,
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text_color": "#ffffff",
      "background_color": "#000000"
    },

    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "SSH Passphrase:"
      }
    },

    {
      "type": "EditText",
      "id": 88801,
      "is_password": true,
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text_color": "#ffffff",
      "background_color": "#000000",
      "text": "password"
    },

    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "SSH Host(name/ip):"
      }
    },
    {
      "type": "EditText",
      "id": 88802,
      "max_length": 40,
      "filter": "no-whitespace",
      "is_single_line": true,
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text": "localhost",
      "text_color": "#ffffff",
      "background_color": "#000000"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "SSH Port:"
      }
    },
    {
      "type": "EditText",
      "id": 88803,
      "max_length": 6,
      "filter": "digits",
      "is_single_line": true,
      "layout_width": "match_parent",
      "layout_height": "wrap_content",
      "text": "8022",
      "text_color": "#ffffff",
      "background_color": "#000000"
    },
    {
      "type": "Clone",
      "name": "HorizontalLine"
    },
    {
      "type": "Clone",
      "name": "TextViewLabel",
      "attributes": {
        "text": "Test SecureShell Connection"
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "whoami",
        "on_click": {
          "type": "PopupTextView",
          "title": "whoami",
          "font": "font/droid-sans-mono/DroidSansMono.ttf",
          "ssh": "whoami"
        }
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "pwd",
        "on_click": {
          "type": "PopupTextView",
          "title": "pwd",
          "font": "font/droid-sans-mono/DroidSansMono.ttf",
          "ssh": "pwd"
        }
      }
    },
    {
      "type": "Clone",
      "name": "ButtonDefault",
      "attributes": {
        "text": "ls",
        "on_click": {
          "type": "PopupTextView",
          "title": "ls",
          "font": "font/droid-sans-mono/DroidSansMono.ttf",
          "ssh": "ls"
        }
      }
    }

  ]
}


